{
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "gitlens.codeLens.enabled": false,
  "metals.javaHome": "/Users/tomas/.sdkman/candidates/java/current",
  "python.formatting.provider": "black",
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.scrollback": 9999,
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "workbench.colorCustomizations": {
    "terminal.background": "#181818",
    "terminal.foreground": "#ABB2BF",
    "terminalCursor.background": "#ABB2BF",
    "terminalCursor.foreground": "#ABB2BF",
    "terminal.ansiBlack": "#282C34",
    "terminal.ansiBlue": "#61AFEF",
    "terminal.ansiBrightBlack": "#545862",
    "terminal.ansiBrightBlue": "#61AFEF",
    "terminal.ansiBrightCyan": "#56B6C2",
    "terminal.ansiBrightGreen": "#98C379",
    "terminal.ansiBrightMagenta": "#C678DD",
    "terminal.ansiBrightRed": "#E06C75",
    "terminal.ansiBrightWhite": "#C8CCD4",
    "terminal.ansiBrightYellow": "#E5C07B",
    "terminal.ansiCyan": "#56B6C2",
    "terminal.ansiGreen": "#98C379",
    "terminal.ansiMagenta": "#C678DD",
    "terminal.ansiRed": "#E06C75",
    "terminal.ansiWhite": "#ABB2BF",
    "terminal.ansiYellow": "#E5C07B"
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "python.terminal.activateEnvironment": false,
  "editor.rulers": [80, 100, 120],
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.fontFamily": "Jetbrains Mono, Menlo, Monaco, 'Courier New', monospace",
  "search.exclude": {
    ".bloop": true,
    ".metals": true,
    "**/metals.sbt": true
  },
  "editor.cursorStyle": "block",
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "python.showStartPage": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Darcula Theme from IntelliJ",
  "editor.columnSelection": false,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.formatting.blackPath": "/Users/tomas/.pyenv/shims/black",
  "editor.formatOnSave": true
}
